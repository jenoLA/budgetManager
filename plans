a simple manager of investiments of mine

structs:
	List
		Node start
		int size
	
	Budget
		char* name
		Paper* start
		int size
		Paper* start

	Node
		struct node* next
		Budget* budget

	Paper
		char* code
		float initialValue
		float finalValue
		time_t brought
		time_t selled
		struct paper* next

functions:
	create List/Budget/Paper
	search Budget/Paper
	save Paper(on budget)/List(on a file)
	delete Paper/budget
	update Paper
	append Paper
	read List # on future

	main budget loop
	main paper loop
